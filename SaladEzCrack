local kavoUi = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local window = kavoUi.CreateLib("script library","BloodTheme")
 
---Tabs
 
local Tab1 = window:NewTab("Blox Fruits")
